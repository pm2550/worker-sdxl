{
  "tests": [
    {
      "name": "basic_test",
      "input": {
        "prompt": "a majestic steampunk dragon soaring through a cloudy sky, intricate clockwork details, golden hour lighting, highly detailed",
        "negative_prompt": "blurry, very low quality, deformed, ugly, text, watermark, signature",
        "height": 128,
        "width": 128,
        "num_inference_steps": 2,
        "refiner_inference_steps": 2,
        "guidance_scale": 7.5,
        "strength": 0.3,
        "high_noise_frac": 0.8,
        "seed": 1337,
        "scheduler": "DDIM",
        "num_images": 1,
        "image_url": null
      },
      "timeout": 180000
    }
  ],
  "config": {
    "gpuTypeId": "NVIDIA RTX A4000",
    "gpuCount": 1,
    "env": [
      {
        "key": "RUNPOD_BUILD_TEST_MODE",
        "value": "1"
      },
      {
        "key": "HEALTH_CHECK_INIT_FAST_MODE",
        "value": "true"
      },
      {
        "key": "HEALTH_CHECK_INTERVAL_SECONDS",
        "value": "5"
      },
      {
        "key": "SCALING_MIN_QUEUE_TIME_MS",
        "value": "1000"
      },
      {
        "key": "SCALING_THRESHOLD_BUFFER_MS",
        "value": "5000"
      }
    ],
    "allowedCudaVersions": [
      "12.7",
      "12.6",
      "12.5",
      "12.4",
      "12.3",
      "12.2",
      "12.1"
    ]
  }
}
