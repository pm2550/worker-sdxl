{
  "tests": [
    {
      "name": "basic_txt2img",
      "input": {
        "prompt": "a small cat sitting on a table",
        "negative_prompt": "blurry, low quality",
        "height": 1024,
        "width": 1024,
        "num_inference_steps": 2,
        "refiner_inference_steps": 2,
        "guidance_scale": 7.5,
        "strength": 0.3,
        "high_noise_frac": 0.8,
        "seed": 42,
        "scheduler": "DDIM",
        "num_images": 1
      },
      "timeout": 300000
    }
  ],
  "config": {
    "gpuTypeId": "NVIDIA RTX A4000",
    "gpuCount": 1,
    "env": [
      {
        "key": "LOCAL_FILES_ONLY",
        "value": "1"
      }
    ],
    "allowedCudaVersions": [
      "12.7",
      "12.6",
      "12.5",
      "12.4",
      "12.3",
      "12.2",
      "12.1"
    ]
  }
}
